{% extends 'base.html.twig' %}

{% block body %}

    <div>
        <h1>Entrez le numéro d'affaire pour retrouver tous les contacts qui y sont liés</h1>
        <form action="{{ path('getProject') }}" method="post">

            <div class="col-md-8">
                <div class="form-group col-lg-8">
                    <label for="refAffaire">Numéro affaire</label>
                    <input id="refAffaire" name="refAffaire" type="text">
                </div>
                <div class="form-group col-lg-4">
                    <input type="submit">
                </div>
            </div>
        </form>

    </div>
    {% endblock %}